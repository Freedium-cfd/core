                }
              ],
              "metadata": null,
              "mixtapeMetadata": null,
              "type": "P",
              "hasDropCap": null,
              "dropCapImage": null,
              "codeBlockMetadata": null
            },
            {
              "__typename": "Paragraph",
              "id": "c570f6584a87_20",
              "name": "447e",
              "href": null,
              "text": "",
              "iframe": null,
