            },
            {
              "__typename": "Paragraph",
              "id": "c570f6584a87_21",
              "name": "5833",
              "href": null,
              "text": "Then, I select Vary (Region):",
              "iframe": null,
              "layout": null,
              "markups": [],
              "metadata": null,
              "mixtapeMetadata": null,
              "type": "P",
              "hasDropCap": null,
              "dropCapImage": null,
              "codeBlockMetadata": null
            },
            {
              "__typename": "Paragraph",
              "id": "c570f6584a87_22",
              "name": "2c31",
              "href": null,
              "text": "",
              "iframe": null,
              "layout": "INSET_CENTER",
              "markups": [],
              "metadata": {
                "__typename": "ImageMetadata",
                "id": "1*Rex_4Jgvkt5N2FyCnDLoaA.png",
