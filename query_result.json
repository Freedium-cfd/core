            },
            {
              "__typename": "Paragraph",
              "id": "c570f6584a87_23",
              "name": "f600",
              "href": null,
              "text": "Clicking on Vary (Region) brings up an editor that has (1) a rectangular select tool, (2) a Lasso select tool, (3) an undo button and (4) a prompt box, and (5) a close button:",
              "iframe": null,
              "layout": null,
              "markups": [],
              "metadata": null,
              "mixtapeMetadata": null,
              "type": "P",
              "hasDropCap": null,
              "dropCapImage": null,
              "codeBlockMetadata": null
            },
            {
              "__typename": "Paragraph",
              "id": "c570f6584a87_24",
              "name": "a0b6",
              "href": null,
              "text": "",
              "iframe": null,
              "layout": "INSET_CENTER",
              "markups": [],
              "metadata": {
                "__typename": "ImageMetadata",
                "id": "1*ilbGCGy8Wchrb7F7RHMxPQ.png",
